   <define name="tei_dddatt.media.attribute.height">
      <optional>
         <attribute name="height">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Where the
               media are displayed, indicates the display height.</a:documentation>
            <data type="token">
               <param name="pattern"
                  >[\-+]?\d+(\.\d+)?(%|cm|mm|in|pt|pc|px|em|ex|ch|rem|vw|vh|vmin|vmax)</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="tei_dddatt.media.attribute.scale">
      <optional>
         <attribute name="scale">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Where the
               media are displayed, indicates a scale factor to be applied when generating the
